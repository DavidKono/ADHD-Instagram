{
    "manifest_version": 3,
    "name": "Instagram Account Bouncer",
    "version": "1.0",
    "description": "Only allows specified accounts to be shown",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://www.instagram.com/*"],
        "js": ["block_accounts.js"]
      }
    ]
  }
  